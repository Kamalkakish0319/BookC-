#include <iostream>
using namespace std;

class Book {
public:
    string title;
    string author;
    double cost;
    int isbn;
    
};

int main() {
    
    Book book1;
    book1.title = "On Tyranny";
    book1.author = "Timothy Snyder";
    book1.cost = 19.99;
    book1.isbn = 13579;
    
    Book book2;
    book2.title = "The DA Vince Code";
    book2.author = "Dan Brown";
    book2.cost = 29.99;
    book2.isbn = 24681;
    
    cout << book1.title << " " << book1.author << " " << book1.cost << " " << book1.isbn << endl;
    cout << book2.title << " " << book2.author << " " << book2.cost << " " << book2.isbn << endl;
    return 0;
    
    
    
    
}
